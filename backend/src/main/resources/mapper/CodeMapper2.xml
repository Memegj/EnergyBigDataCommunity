<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="upc.backend.mapper.CodeMapper3">
    <resultMap id="BaseResultMap" type="upc.backend.entity.Code">
        <id column="CodeId" jdbcType="INTEGER" property="CodeId" />
        <result column="CodeName" jdbcType="VARCHAR" property="CodeName" />
        <result column="UploadTime" jdbcType="TIMESTAMP" property="UploadTime" />
        <result column="CodeAbstract" jdbcType="VARCHAR" property="CodeAbstract" />
        <result column="CodeDetails" jdbcType="VARCHAR" property="CodeDetails" />
        <result column="isPublic" jdbcType="INTEGER" property="isPublic" />
        <result column="DownLoadTimes" jdbcType="INTEGER" property="DownLoadTimes" />
        <result column="UserId" jdbcType="INTEGER" property="UserId" />
        <result column="Url" jdbcType="VARCHAR" property="Url" />
        <association property="user" javaType="upc.backend.entity.User">
            <id column="UserId" jdbcType="INTEGER" property="UserId" />
            <!-- 这里假设User实体的主键也是id，如果不是，请修改为实际的主键字段名 -->
        </association>
    </resultMap>
    <resultMap id="BaseResultMap" type="upc.backend.entity.File">
        <id column="FileId" jdbcType="INTEGER" property="FileId" />
    </resultMap>
    <resultMap id="FullResultMap" extends="BaseResultMap" type="upc.backend.entity.File">
        <association property="data" javaType="upc.backend.entity.Data">
            <id column="DataId" jdbcType="INTEGER" property="DataId" />
        </association>
        <association property="video" javaType="upc.backend.entity.Video">
            <id column="VideoId" jdbcType="INTEGER" property="VideoId" />
        </association>
        <association property="liter" javaType="upc.backend.entity.Liter">
            <id column="LiterId" jdbcType="INTEGER" property="LiterId" />
        </association>
        <association property="code" javaType="upc.backend.entity.Code">
            <id column="CodeId" jdbcType="INTEGER" property="CodeId" />
        </association>
        <association property="team" javaType="upc.backend.entity.Team">
            <id column="TeamId" jdbcType="INTEGER" property="TeamId" />
        </association>
    </resultMap>
    <sql id="Base_Column_List">
        CodeId,CodeName,UploadTime,CodeAbstract,CodeDetails,isPublic,DownLoadTimes,UserId,Url
    </sql>

    <select id="selectByID" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from code
        where CodeId = #{CodeId,jdbcType=INTEGER}
    </select>

    <select id="selectByTitle" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from code
        where CodeName= #{CodeName,jdbcType=VARCHAR}
    </select>
    <select id="getNumOfTotalReferences" parameterType="Map" resultType="int">
        select count(*) from code
    </select>
    <select id="findAllReferenceList" parameterType="Map" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from code
        <if test="start!=null and limit!=null">
            limit #{start},#{limit}
        </if>
    </select>
    <insert id="insert" parameterType="upc.backend.entity.Code">
        insert into code(CodeId,CodeName,UploadTime,CodeAbstract,CodeDetails,isPublic,DownLoadTimes,UserId,Url)
        values (#{excelid,jdbcType=INTEGER}, #{excelname,jdbcType=VARCHAR}, #{upload_time,jdbcType=TIMESTAMP}, #{upload_user,jdbcType=VARCHAR})
    </insert>
    <insert id="insertSelective" parameterType="upc.backend.entity.Excel">
        insert into excel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="excelid != null">
                excelid,
            </if>
            <if test="excelname != null">
                excelname,
            </if>

            <if test="upload_time != null">
                upload_time,
            </if>
            <if test="upload_user!= null">
                upload_user,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="excelid != null">
                #{excelid, jdbcType=INTEGER},
            </if>
            <if test="excelname != null">
                #{excelname, jdbcType=VARCHAR},
            </if>

            <if test="upload_time != null">
                #{upload_time, jdbcType=TIMESTAMP},
            </if>
            <if test="upload_user!= null">
                #{upload_user, jdbcType=VARCHAR},
            </if>

        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="upc.backend.entity.Excel">
        update excel
        <set>
            <if test="excelname != null">
                excelname= #{excelname,jdbcType=VARCHAR},
            </if>
            <if test="upload_time != null">
                upload_time = #{upload_time,jdbcType=TIMESTAMP},
            </if>
            <if test="upload_user != null">
                upload_user = #{upload_user,jdbcType=VARCHAR},
            </if>
        </set>
        where excelid = #{excelid,jdbcType=INTEGER}
    </update>
    <update id="updateByPrimaryKey" parameterType="upc.backend.entity.Excel">
        update excel
        set excelname= #{excelname,jdbcType=VARCHAR},
            upload_time = #{upload_time,jdbcType=TIMESTAMP},
            upload_user = #{upload_user,jdbcType=VARCHAR},
        where excelid = #{excelid,jdbcType=INTEGER}
    </update>
    <delete id="deleteBatch" parameterType="java.lang.Integer">
        delete from excel
        where excelid in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>